<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androidrion.praktikumsqlite">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.androidrion.praktikumsqlite.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.androidrion.praktikumsqlite.BuatBiodata"
            android:label="Form Biodata Baru">
        </activity>
        <activity android:name="com.androidrion.praktikumsqlite.LihatBiodata"
            android:label="Lihat Biodata">
        </activity>
        <activity android:name="com.androidrion.praktikumsqlite.UpdateBiodata"
            android:label="Update Biodata">
        </activity>
    </application>
</manifest>